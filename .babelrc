{
  "presets": ["react", "es2015"],
  "plugins": [
    ["import", { "libraryName": "antd", "style": "css" }],"recharts"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",

            "imports": ["react"],

            "locals": ["module"]
          }]
        }]
      ] }
  }
}


